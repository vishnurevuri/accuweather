					<exclude
						name="tc19_PDP_AnimationGiftCard_AddToCart_Products_RecipientEmail_GiftMessage_TO_FROM_Message_Cleared" />
					<exclude
						name="tc21_PDP_AnimationGiftCard_RecipientEmail_GiftMessage_TO_FROM_Message_Retained_When_Another_Product_Selected_From_PLP" />
					<exclude
						name="tc23_PDP_AnimationGiftCard_AddToCart_Products_RecipientEmail_GiftMessage_TO_FROM_Message_Not_Retained_When_Another_Product_selected_From_PLP" />
					<exclude
						name="tc25_PDP_AnimationGiftCard_GuestUser_RecipientEmail_GiftMessage_TO_FROM_Message_Retained_After_Login" />
					<exclude
						name="tc27_PDP_AnimationGiftCard_GuestUser_RecipientEmail_GiftMessage_TO_FROM_Message_Message_NotRetained_After_Login_When_AddedToCart" />
					<exclude
						name="tc29_PDP_AnimationGiftCard_SignedInUser_RecipientEmail_GiftMessage_TO_FROM_Message_Retained_AfterLogout" />
					<exclude
						name="tc31_PDP_AnimationGiftCard_SignedInUser_RecipientEmail_GiftMessage_TO_FROM_Message_NotRetained_After_Logout_When_AddedToCart" />
				</methods>
			</class>
		</classes>
	</test>

