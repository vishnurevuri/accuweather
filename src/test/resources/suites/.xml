	<test name="PDP Gift Message Retaining Info Test">
		<parameter name="browserName" value="chrome" />
		<classes>
			<class
				name="com.incomm.vanillabolsf.tests.PDPGiftMessageRetainingInfoTest">
				<methods>
					<exclude
